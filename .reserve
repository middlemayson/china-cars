<!-- Это вызов одного бренда -->

<div className="container">
                <Brand title='Aston Martin' desc='17% акций компании' linkDiller='#!' linkDetails='#!' imgLogo={imgLogoAston} imgCar={imgPhotoAston}/>
            </div>

<!-- Это структура одного бренда -->

            <div className="card">
                <img className="card__img" src={imgCar} alt={title} />
                <div className="card__body">
                    <img className='card__imgLogo' src={imgLogo} alt={title} />
                    <div className="card__text">
                        <div className="card__title">
                            {title}
                        </div>
                        <div className="card__muted">
                            {desc}
                        </div>
                        
                    </div>
                    
                </div>
                <div className="card__btn-sector">
                            <a href={linkDetails} className="btn">Подробнее</a>
                            <a href={linkDiller} className="btn btn-red">Диллер в РК</a>
                    </div>
            </div>

            Это структура одной компании

            <section className="company-element">
            <div className="container">
                
                <div className="brand__title">
                    <img className="brand__title__img" src={logo} alt="" />
                    <h1 className="title">{title}</h1>
                </div>
                <img src={photo} alt="" className="brandPhoto" />
                <p className="brand__text">{desc}</p>
            </div>
            
        </section>

        Это кнопка назад

        <div className="brand__nav">
                <button className="btn-circle back-icon"></button>
                <p>Вернуться на главную</p>
            </div>


Это главная страница на пк

.company-element {
    width: calc(100%/3 - 14px);
}